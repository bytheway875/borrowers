<div class="card friend-profile">
  <div class= "friend-info full">
    {{fill-murray width=150 height=150}}
  </div>
  <p> {{model.fullName}}</p>
  <p> {{model.email}}</p>
  <p> {{model.twitter}}</p>
  <ul class= "friend-profile-links">
    <li> {{link-to "Edit" "friends.edit" model}} </li>
    <li> <a href="#" {{action "delete" model}}>delete</a></li>
  </ul>
</div>
<div class="articles-container">
  <!-- any nested route is by default rendered into its parents outlet -->
  {{outlet}}
  {{link-to "Lend articles" "articles.new" model}}
</div>
